<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.natasha.voting.controllers.VotingAppController">
   <children>
      <Pane prefHeight="263.0" prefWidth="306.0">
         <children>
             <Button alignment="TOP_LEFT" mnemonicParsing="false" onAction="#onHomeButtonClick" text="Home">
                 <cursor>
                     <Cursor fx:constant="HAND" />
                 </cursor>
             </Button>
            <ButtonBar buttonOrder="" layoutX="27.0" layoutY="132.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="253.0">
              <buttons>
                   <Button alignment="CENTER" onAction="#onCandidateOneButtonClick" prefHeight="25.0" prefWidth="117.0" text="Candidate One" translateX="-15.0">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                <Button alignment="CENTER" mnemonicParsing="false" onAction="#onCandidateTwoButtonClick" text="Candidate Two" translateX="-10.0">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
              </buttons>
            </ButtonBar>
            <Label fx:id="labelText" alignment="CENTER" layoutX="26.0" layoutY="96.0" prefHeight="19.0" prefWidth="222.0" text="Who would you like to vote for?">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         
                   <Label fx:id="candidateSelectionText" alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="195.0" prefHeight="17.0" prefWidth="222.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
